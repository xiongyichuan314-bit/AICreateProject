{
  "name": "dynamic-data-api",
  "version": "2.0.0",
  "description": "Dynamic Data API Server - 重构版本",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon api/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "clean": "rm -rf logs/*.log coverage",
    "prestart": "npm run lint",
    "pretest": "npm run lint"
  },
  "keywords": [
    "api",
    "rest",
    "express",
    "sqlite",
    "crud"
