    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "imap": "^0.8.19",
    "mailparser": "^3.9.3",
    "nodemailer": "^8.0.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
